#include <iostream>
#include <string>
using namespace std;
int main(int argc, char **argv)
{
	int x, i, j;
	cout<<"введите количество верблюдов"<<endl;
	cin>>x;
	string a = "верблюд";
	string b;
	i = x%10;
	j = x%100;
	if (((i==0)||(i>=5 && i<=9)) ||(j>=11 && j<=14)) {
		a = a.append("ов");
	} else {
		if (i==1) {
			a = a.append(b);
		} else {
			a = a.append("a");
		}
	}
	cout << "В караване было " << x << " " << a << endl;
	return 0;
}
